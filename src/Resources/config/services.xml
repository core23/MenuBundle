<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="core23_menu.builder.config" class="Core23\MenuBundle\Menu\ConfigBuilder" public="false">
      <argument type="service" id="knp_menu.factory"/>
      <argument type="service" id="translator"/>
    </service>
    <service id="core23_menu.config_provider" class="Core23\MenuBundle\Provider\ConfigProvider" public="false">
      <tag name="knp_menu.provider"/>
      <argument type="service" id="core23_menu.builder.config"/>
      <argument>%core23_menu.groups%</argument>
    </service>
  </services>
</container>
