{
  "name": "core23/menu-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides services for defining static menus for sonata.",
  "keywords": [
    "symfony",
    "sonata",
    "sonata-block",
    "bundle",
    "menu",
    "knpmenu",
    "generator"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.3",
    "knplabs/knp-menu": "^2.1",
    "sonata-project/block-bundle": "^4.0",
    "symfony/config": "^4.2 || ^5.0",
    "symfony/dependency-injection": "^4.2 || ^5.0",
    "symfony/expression-language": "^4.2 || ^5.0",
    "symfony/framework-bundle": "^4.2 || ^5.0",
    "symfony/http-kernel": "^4.2 || ^5.0",
    "symfony/translation-contracts": "^1.1 || ^2.0"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.3",
    "ergebnis/composer-normalize": "^2.0.1",
    "symfony/translation": "^4.2 || ^5.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Core23\\MenuBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Core23\\MenuBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer bin all install --ansi"
    ],
    "post-update-cmd": [
      "@composer bin all update --ansi"
    ]
  }
}
